<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>pmml-model-parent</artifactId>
    <groupId>org.manstis</groupId>
    <version>0.0.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <artifactId>pmml-model-distribution</artifactId>
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-shade-plugin</artifactId>
          <version>${version.org.apache.maven.plugins.maven-shade-plugin}</version>
        </plugin>
      </plugins>
    </pluginManagement>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <artifactSet>
                <excludes>
                  <exclude>org.jvnet.jaxb2.maven2:maven-jaxb2-plugin</exclude>
                  <exclude>org.jvnet.jaxb2.maven2:maven-jaxb2-plugin-core</exclude>
                  <exclude>org.slf4j:slf4j-api</exclude>
                  <exclude>org.apache.commons:commons-lang3</exclude>
                  <exclude>com.sun.org.apache.xml.internal:resolver</exclude>
                  <exclude>org.glassfish.jaxb:txw2</exclude>
                  <exclude>org.apache.maven:maven-plugin-api</exclude>
                  <exclude>org.sonatype.plexus:plexus-build-api</exclude>
                  <exclude>org.apache.maven:maven-project</exclude>
                  <exclude>org.apache.maven:maven-settings</exclude>
                  <exclude>org.apache.maven:maven-profile</exclude>
                  <exclude>org.apache.maven:maven-model</exclude>
                  <exclude>org.apache.maven:maven-artifact-manager</exclude>
                  <exclude>org.apache.maven:maven-repository-metadata</exclude>
                  <exclude>org.apache.maven.wagon:wagon-provider-api</exclude>
                  <exclude>org.apache.maven:maven-plugin-registry</exclude>
                  <exclude>org.codehaus.plexus:plexus-container-default</exclude>
                  <exclude>junit:junit</exclude>
                  <exclude>classworlds:classworlds</exclude>
                  <exclude>org.codehaus.plexus:plexus-utils</exclude>
                  <exclude>org.jvnet.jaxb2.maven2:maven-jaxb23-plugin</exclude>
                  <exclude>org.glassfish.jaxb:jaxb-runtime</exclude>
                  <exclude>org.glassfish.jaxb:jaxb-core</exclude>
                  <exclude>org.jvnet.staxex:stax-ex</exclude>
                  <exclude>com.sun.xml.fastinfoset:FastInfoset</exclude>
                  <exclude>org.glassfish.jaxb:jaxb-xjc-jdk9</exclude>
                  <exclude>org.glassfish.jaxb:jaxb-xjc</exclude>
                  <exclude>com.sun.xml.dtd-parser:dtd-parser</exclude>
                  <exclude>org.glassfish.jaxb:xsom</exclude>
                  <exclude>org.glassfish.jaxb:codemodel</exclude>
                  <exclude>com.sun.xml.bind.external</exclude>
                  <exclude>com.sun.istack:istack-commons-tools</exclude>
                  <exclude>org.apache.ant:ant</exclude>
                  <exclude>org.apache.ant:ant-launcher</exclude>
                  <exclude>com.sun.istack:istack-commons-runtime</exclude>
                  <exclude>relaxngDatatype:relaxngDatatype</exclude>
                  <exclude>com.sun.activation:javax.activation</exclude>
                  <exclude>org.apache.maven.plugin-tools:maven-plugin-annotations</exclude>
                  <exclude>org.apache.maven:maven-artifact</exclude>
                  <exclude>org.jvnet.jaxb2_commons:jaxb2-basics</exclude>
                  <exclude>org.jvnet.jaxb2_commons:jaxb2-basics-runtime</exclude>
                  <exclude>org.jvnet.jaxb2_commons:jaxb2-basics-tools</exclude>
                  <exclude>commons-beanutils:commons-beanutils</exclude>
                  <exclude>commons-collections:commons-collections</exclude>
                  <exclude>org.slf4j:jcl-over-slf4j</exclude>
                  <exclude>com.google.code.javaparser:javaparser</exclude>
                  <exclude>org.jvnet.jaxb2_commons:jaxb2-basics-annotate</exclude>
                  <exclude>org.jvnet.annox:annox</exclude>
                  <exclude>commons-io:commons-io</exclude>
                  <exclude>com.google.jsinterop:base</exclude>
                  <exclude>com.vertispan.jsinterop:base</exclude>
                  <exclude>com.google.jsinterop:jsinterop-annotations</exclude>
                  <exclude>com.vertispan.j2cl:gwt-internal-annotations</exclude>
                </excludes>
              </artifactSet>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>
